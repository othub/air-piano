void drawWhiteKeys() {
  // key C1
  fill( c1, 255, 255);
  drawWhiteKey(190, "C1");

  // key D1
  fill(d1, 255, 255);
  drawWhiteKey(300, "D1");

  // key E1
  fill(e1, 255, 255);
  drawWhiteKey(410, "E1");

  // key F1
  fill(f1, 255, 255);
  drawWhiteKey(520, "F1");

  // key G1
  fill(g1, 255, 255);
  drawWhiteKey(630, "G1");

  // key A1
  fill(a1, 255, 255);
  drawWhiteKey(740, "A1");

  // key B1
  fill(b1, 255, 255);
  drawWhiteKey(850, "B1");

  // key C2
  fill(c2, 255, 255);
  drawWhiteKey(960, "C2");

  // key D2
  fill(d2, 255, 255);
  drawWhiteKey(1070, "D2");

  // key E2
  fill(e2, 255, 255);
  drawWhiteKey(1180, "E2");

  // key F2
  fill(f2, 255, 255);
  drawWhiteKey(1290, "F2");

  // key G2
  fill(g2, 255, 255);
  drawWhiteKey(1400, "G2");

  // key A2
  fill(a2, 255, 255);
  drawWhiteKey(1510, "A2");

  // key B2
  fill(b2, 255, 255);
  drawWhiteKey(1620, "B2");
}

void drawBlackKeys() {
  // key C1#
  fill(0, c1_sharp, c1_sharp);
  drawBlackKey(190, "C1\n #");

  // key D1#
  fill( 0, d1_sharp, d1_sharp);
  drawBlackKey(300, "D1\n #");

  // key F1#
  fill(0, f1_sharp, f1_sharp);
  drawBlackKey(520, "F1\n #");

  // key G1#
  fill( 0, g1_sharp, g1_sharp);
  drawBlackKey(630, "G1\n #");

  // key A1#
  fill(0, a1_sharp, a1_sharp);
  drawBlackKey(740, "A1\n #");

  // key C2#
  fill(0, c2_sharp, c2_sharp);
  drawBlackKey(960, "C2\n #");

  // key D2#
  fill(0, d2_sharp, d2_sharp);
  drawBlackKey(1070, "D2\n #");

  // key F2#
  fill(0, f2_sharp, f2_sharp);
  drawBlackKey(1290, "F2\n #");

  // key G2#
  fill(0, g2_sharp, g2_sharp);
  drawBlackKey(1400, "G2\n #");

  // key A2#
  fill(0, a2_sharp, a2_sharp);
  drawBlackKey(1510, "A2\n #");
}
