  // key E1
  fill(e1, 255, 255);
  drawWhiteKey(330);

  // key F1
  fill(f1, 255, 255);
  drawWhiteKey(440);

  // key G1
  fill(g1, 255, 255);
  drawWhiteKey(550);

  // key A1
  fill(a1, 255, 255);
  drawWhiteKey(660);

  // key B1
