  drawWhiteKey(300);

  // key E1
  fill(e1, 255, 255);
  drawWhiteKey(410);

  // key F1
  fill(f1, 255, 255);
  drawWhiteKey(520);

  // key G1
  fill(g1, 255, 255);
  drawWhiteKey(630);

  // key A1
  fill(a1, 255, 255);
  drawWhiteKey(740);

  // key B1
  fill(b1, 255, 255);
  drawWhiteKey(850);

  // key C2
  fill(c2, 255, 255);
